{
  "name": "gulp-happy-starter",
  "version": "0.0.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/infernalmaster/gulp-happy-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "2.7.7",
    "browserify": "10.2.4",
    "browserify-shim": "^3.8.2",
    "coffeeify": "1.1.0",
    "del": "1.2.0",
    "glob": "5.0.10",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "1.2.1",
    "gulp-coffeelint": "0.5.0",
    "gulp-csso": "1.0.0",
    "gulp-if": "^1.2.4",
    "gulp-imagemin": "2.2.1",
    "gulp-jade": "1.0.1",
    "gulp-minify-html": "1.0.3",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.0.1",
    "gulp-replace": "0.5.3",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "1.2.0",
    "gulp-uncss": "1.0.1",
    "gulp-useref": "1.2.0",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "1.0.0",
    "require-dir": "0.3.0",
    "run-sequence": "1.1.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.2.2"
  }
